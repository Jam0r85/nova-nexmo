{
    "name": "jam0r85/nova-nexmo",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Jam0r85\\NovaNexmo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jam0r85\\NovaNexmo\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
